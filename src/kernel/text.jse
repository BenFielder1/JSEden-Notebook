func Text{
    ${{
        let t = arguments[0];
        let x = arguments[1];
        let y = arguments[2];
        let fs = arguments[3];
        let f = arguments[4];

        if(fs === undefined) fs = 16;
        if(f === undefined) f = "white";

        return `<text x="${x}" y="${y}" fill="${f}" font-size="${fs}">${t}</text>`
    }}$;
};