func CounterOriginal{
    ${{
        t = 0;
        setInterval(()=>{
            context.lookup(arguments[0]).assign(t++,context.scope,"Timer");
        },arguments[1]);
    }}$;
};

func Counter{
    ${{
        t = 0;
        setInterval(()=>{
            arguments[0].assign(t++,context.scope,"Timer");
        },arguments[1]);
    }}$;
};